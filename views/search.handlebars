<div class="searchDiv">
    <form action="" method="post">
        <input type="text" name="search">
        <button type="submit">search</button>
        <input type="hidden" name="_csrf" value="{{csrfToken}}">
    </form>
</div>

<div>
    <ul class="searchlist">
        {{#users}}
        {{#.}}
        <li>
            <a href="/edit/user/{{id}}">{{first_name}} &nbsp;{{last_name}}</a>
        </li>
        {{/.}}
        {{/users}}
    </ul>
</div>